   "source": [
    "s = \"abscesrhsjfhksjdfhke\"\n",
    "k = 3\n",
    "fill = \"$\"\n",
    "# it works like this range(start, stop, step)\n",
    "groups = [s[ i : i+ k] for i in range(0, len(s), k )]\n",
    "print(groups)\n",
    "if len(groups[-1]) < k:\n",
    "    groups[-1] = groups[-1].ljust(k, fill)\n",
    "print(groups)"
   ]
